<div id="<%= dom_id blog %>">
  <p>
    <strong>Title:</strong>
    <%= blog.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= blog.body %>
  </p>

  <p>
    <strong>Orig date:</strong>
    <%= blog.orig_date %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= blog.status %>
  </p>

  <p>
    <strong>Featured:</strong>
    <%= blog.featured %>
  </p>

</div>
